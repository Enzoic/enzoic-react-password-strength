    "node": ">6.11.1"
  },
  "description": "PasswordPing password strength meter control for use in React projects",
  "main": "dist/index.js",
  "scripts": {
    "build-module": "./node_modules/.bin/webpack --config=webpack/webpack.build.config.js",
