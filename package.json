  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jasmine": "^2.8.0",
